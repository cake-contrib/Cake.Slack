; This file is for unifying the coding style for different editors and IDEs.
; More information at http://EditorConfig.org

root = true

[*]
end_of_line = CRLF
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.{bat,ps1}]
charset = utf-8-bom
end_of_line = crlf
indent_style = space
indent_size = 4

[*.cake]
indent_style = space
indent_size = 4

[*.js]
indent_style = tab
indent_size = 2
